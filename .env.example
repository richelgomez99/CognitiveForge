# Google Gemini API
GOOGLE_API_KEY=your_google_api_key_here

# Neo4j Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password123

# System Configuration
MAX_ITERATIONS=3

# Per-Agent Model Configuration (Tier 1 - FR-T1-014/015)
# Updated to use Gemini 2.x models (1.5 variants deprecated)
GEMINI_MODEL_ANALYST=gemini-2.5-pro
GEMINI_MODEL_SKEPTIC=gemini-2.5-flash
GEMINI_MODEL_SYNTHESIZER=gemini-2.5-pro

# Global Model Fallback
GEMINI_MODEL=gemini-2.5-pro

# FastAPI Authentication (Tier 2 - FR-T2-012)
# This is YOUR custom key - create any secure string you want
# Example: API_KEY=cogforge_dev_secret_2024
API_KEY=your_api_key_here

# Development Settings
DEBUG=true
LOG_LEVEL=INFO
